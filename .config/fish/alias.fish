alias vim "nvim"
alias ls "eza --icons"
alias l "eza --icons -a"
alias lt "eza --icons --tree -a"
alias denable "$HOME/Coding/bash/enable_debounce.sh"
alias ddisable "$HOME/Coding/bash/disable_debounce.sh"

# XDG
alias wget "wget --hsts-file='$XDG_DATA_HOME/wget-hsts'"
alias mocp "mocp -M $XDG_CONFIG_HOME/moc"
alias gpg "gpg --homedir $HOME/.local/share/gnupg"
